FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceCoefficients;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

functions
{
    frictionCoefficient
    {
        type            frictionCoefficient;
        libs            ("libstrathFieldFunctionObjects.so");
        writeControl    outputTime;
        log             no;
    }

    pressureCoefficient
    {
        type            pressureCoefficient;
        libs            ("libstrathFieldFunctionObjects.so");
        writeControl    outputTime;
        log             no;
    }

    yPlus
    {
        type            yPlus;
        libs            ("libstrathFieldFunctionObjects.so");
        writeControl    outputTime;
        log             no;
    }
}


