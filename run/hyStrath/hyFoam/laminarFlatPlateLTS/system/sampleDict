FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat       raw;

sets
(
    line
    {
        type    face;
        axis    y;
        start   ( 0.05 0 0 );
        end     ( 0.05 0.005 0 );
        nPoints 1000;
    }
);

fields          (p Tt rho U);

