FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "5e-05";
    object      N2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.76708;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.76708;
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            symmetry;
    }
    top
    {
        type            zeroGradient;
    }
    plate
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


