FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pdFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         3e-5;

deltaT          2.5e-9;

writeControl    runTime;

writeInterval   1.5e-5;

purgeWrite      0;

writeFormat     ascii;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no;

functions
{

}

