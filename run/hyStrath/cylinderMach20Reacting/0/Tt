FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Tt;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 220;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 220;
    }
    cylinder
    {
        type            nonEqSmoluchowskiJumpT;
        accommodationCoeff 1;
        Twall           uniform 1000;
        value           uniform 1000;
    }
    axis
    {
        type            symmetry;
    }
    outlet
    {
        type            zeroGradient;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


