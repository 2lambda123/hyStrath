{
    FrontBack
    {
        type            empty;
    }
    OF_inlet_00
    {
        type            fixedValue;
		    value			      uniform 370.0;
    }
    OF_outlet_00
    {
        type            zeroGradient;
    }
    OF_wall_00
    {
		    type            fixedValue;
		    value			      uniform 300.0;
    }
    OF_symmetry_00
    {
        type            symmetry;
    }
