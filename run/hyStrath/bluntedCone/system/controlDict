FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     hy2Foam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.0002;

deltaT          1.0e-10;

writeControl    runTime;

writeInterval   1e-5;

purgeWrite      50;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.2;

maxDeltaT       1.0e-5;

libs ("libstrathFiniteVolume.so");

