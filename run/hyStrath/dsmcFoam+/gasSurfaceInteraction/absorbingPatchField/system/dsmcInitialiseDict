FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      dsmcInitialiseDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

configurations
(
    configuration
    {
        type			dsmcMeshFill;

		    numberDensities
		    {
			      Ar          1.0e21;
		    };

		    translationalTemperature     	2000;
		    rotationalTemperature			0;
		    vibrationalTemperature			0;
            electronicTemperature           0;

		    velocity        (0 0 0);
	  }
);

