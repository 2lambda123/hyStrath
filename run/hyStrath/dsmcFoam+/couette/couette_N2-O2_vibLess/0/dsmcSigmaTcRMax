FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      dsmcSigmaTcRMax;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];


internalField   uniform 5.9534306701856e-16;

boundaryField
{
    upperWall
    {
        type            zeroGradient;
    }
    lowerWall
    {
        type            zeroGradient;
    }
    periodicX_half0
    {
        type            cyclic;
    }
    periodicX_half1
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}


