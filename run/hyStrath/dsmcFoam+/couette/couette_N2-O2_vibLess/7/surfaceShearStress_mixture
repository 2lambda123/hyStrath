boundaryField
{
    upperWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(0.02369308057 0.02299205958 0.02331907135 0.02315534987 0.02355417431);
    }
    lowerWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(0.02323964101 0.02359158014 0.02316230627 0.02296860949 0.02376652802);
    }
    periodicX_half0
    {
        type            cyclic;
    }
    periodicX_half1
    {
        type            cyclic;
