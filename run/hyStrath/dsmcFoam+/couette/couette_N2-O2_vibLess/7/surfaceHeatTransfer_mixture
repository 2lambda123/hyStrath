boundaryField
{
    upperWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(-101.6923548 -101.0863571 -101.4273444 -100.9327319 -101.4032833);
    }
    lowerWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(101.4928822 101.0461929 101.1397707 100.9025951 101.9630944);
    }
    periodicX_half0
    {
        type            cyclic;
    }
    periodicX_half1
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
