FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "8";
    object      surfaceShearStress_mixture;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    upperWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(0.02309947038 0.02327783976 0.02375006547 0.02323976079 0.02323662796);
    }
    lowerWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(0.02336499833 0.02309829864 0.023522175 0.02354462989 0.02309604309);
    }
    periodicX_half0
    {
        type            cyclic;
    }
    periodicX_half1
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}


