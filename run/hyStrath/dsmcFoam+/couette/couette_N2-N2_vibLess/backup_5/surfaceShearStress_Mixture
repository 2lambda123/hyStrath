FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "5";
    object      surfaceShearStress_Mixture;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    upperWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(0.02198427166 0.0225871205 0.02308141163 0.02280779 0.02174628931);
    }
    lowerWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(0.02273095755 0.02245487978 0.02210411674 0.0223831032 0.02253171135);
    }
    periodicX_half0
    {
        type            cyclic;
    }
    periodicX_half1
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}


