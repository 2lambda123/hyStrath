FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "5";
    object      heatFluxVibVector_O2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    upperWall
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    lowerWall
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    periodicX_half0
    {
        type            cyclic;
    }
    periodicX_half1
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}


