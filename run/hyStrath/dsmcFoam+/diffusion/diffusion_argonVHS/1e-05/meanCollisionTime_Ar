FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1e-05";
    object      meanCollisionTime_Ar;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 1 0 0 0 0];


internalField   uniform 2.429328876e-07;

boundaryField
{
    bottom
    {
        type            calculated;
        value           uniform 2.429328876e-07;
    }
    top
    {
        type            calculated;
        value           uniform 2.429328876e-07;
    }
    inlet
    {
        type            calculated;
        value           uniform 2.429328876e-07;
    }
    outlet
    {
        type            calculated;
        value           uniform 2.429328876e-07;
    }
    back
    {
        type            calculated;
        value           uniform 2.429328876e-07;
    }
    front
    {
        type            calculated;
        value           uniform 2.429328876e-07;
    }
}


