FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.008";
    object      rotationalT_Ar;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];


internalField   uniform 0;

boundaryField
{
    flow
    {
        type            calculated;
        value           uniform 0;
    }
    cylinder
    {
        type            calculated;
        value           uniform 0;
    }
    wedgeFront
    {
        type            symmetry;
    }
    wedgeBack
    {
        type            symmetry;
    }
    defaultFaces
    {
        type            empty;
    }
}


