FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "5";
    object      surfaceShearStress_mixture;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    upperWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(0.02317616724 0.02313366438 0.02345496957 0.02331729832 0.02343942206);
    }
    lowerWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(0.02311600393 0.02333256887 0.02345682117 0.02329128929 0.02332836685);
    }
    periodicX_half0
    {
        type            cyclic;
    }
    periodicX_half1
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}


