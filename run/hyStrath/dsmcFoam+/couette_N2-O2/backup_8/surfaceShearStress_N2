boundaryField
{
    upperWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(0.01782435661 0.01824711316 0.01842132286 0.01798000959 0.01805768836);
    }
    lowerWall
    {
        type            calculated;
        value           nonuniform List<scalar> 5(0.01804860615 0.01792229568 0.01803101799 0.01824118565 0.01782963678);
    }
    periodicX_half0
    {
        type            cyclic;
    }
    periodicX_half1
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
