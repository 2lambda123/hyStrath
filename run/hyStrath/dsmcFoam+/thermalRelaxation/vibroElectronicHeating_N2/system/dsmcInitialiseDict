FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      dsmcInitialiseDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
configurations
(
    configuration
    {
        type    dsmcMeshFill;

		    numberDensities
		    {
            N2                1e23;
		    };

		    translationalTemperature     10000;
		    rotationalTemperature        10000; 
		    vibrationalTemperature           0;
		    electronicTemperature            0;

		    velocity        (0 0 0);
    }
);

